{
  "name": "artnews",
  "version": "1.0.0",
  "description": "Art News Scraper",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kathryn-breslin/artnews.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "express": "^4.16.3"
  },
  "bugs": {
    "url": "https://github.com/kathryn-breslin/artnews/issues"
  },
  "homepage": "https://github.com/kathryn-breslin/artnews#readme"
}
